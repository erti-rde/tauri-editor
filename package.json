{
	"name": "citation-machine",
	"version": "0.2.5",
	"private": true,
	"scripts": {
		"tauri": "tauri",
		"dev": "vite dev",
		"dev:backend": "cd src-backend && uvicorn server:app --reload --port 8135",
		"build": "vite build",
		"build:fastapi": "pyinstaller --collect-all en_core_web_sm -c -F --clean --name api-$(rustc -Vv | grep host | cut -f2 -d' ') --distpath src-tauri/bin src-backend/server.py",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@iconify-json/lucide": "^1.1.208",
		"@iconify-json/mdi": "^1.1.68",
		"@iconify-json/ph": "^1.1.14",
		"@internationalized/date": "^3.7.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@tauri-apps/api": "^2.0.3",
		"@tauri-apps/cli": "^2.0.4",
		"bits-ui": "^1.3.11",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"typescript-eslint": "^8.20.0",
		"unplugin-icons": "^0.19.2",
		"vite": "^6.0.0"
	},
	"type": "module",
	"dependencies": {
		"@tauri-apps/plugin-dialog": "~2.0.1",
		"@tauri-apps/plugin-fs": "~2.0.2",
		"@tauri-apps/plugin-http": "~2",
		"@tauri-apps/plugin-shell": "~2",
		"@tauri-apps/plugin-sql": "~2",
		"@tauri-apps/plugin-store": "~2.1.0",
		"@tauri-apps/plugin-updater": "~2",
		"@tiptap/core": "^2.10.3",
		"@tiptap/extension-bubble-menu": "^2.10.3",
		"@tiptap/extension-text-align": "^2.10.3",
		"@tiptap/pm": "^2.10.3",
		"@tiptap/starter-kit": "^2.10.3",
		"@tiptap/suggestion": "^2.11.5",
		"citeproc": "^2.4.63",
		"llm-chunk": "^0.0.1",
		"pdfjs-dist": "4.9.155",
		"tippy.js": "^6.3.7"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
