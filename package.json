{
	"name": "citation-machine",
	"version": "0.2.5",
	"private": true,
	"scripts": {
		"tauri": "tauri",
		"dev": "pnpm dev:backend & vite dev",
		"dev:backend": "cd src-backend && uvicorn server:app --reload --port 8135",
		"build": "vite build",
		"build:fastapi": "pyinstaller --collect-all en_core_web_sm -c -F --clean --name api-$(rustc -Vv | grep host | cut -f2 -d' ') --distpath src-tauri/bin src-backend/server.py",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@iconify-json/lucide": "^1.1.208",
		"@iconify-json/mdi": "^1.1.68",
		"@iconify-json/ph": "^1.1.14",
		"@melt-ui/pp": "^0.3.0",
		"@melt-ui/svelte": "^0.76.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"@tauri-apps/api": "^1.5.3",
		"@tauri-apps/cli": "^1.5.10",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.16",
		"cssnano": "^6.1.0",
		"editorjs-undo": "^2.0.28",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unplugin-icons": "^0.19.2",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@editorjs/editorjs": "^2.29.1",
		"@editorjs/header": "^2.8.1",
		"@editorjs/list": "^1.9.0",
		"iconify-icon": "^2.1.0"
	}
}
