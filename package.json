{
	"name": "citation-machine",
	"version": "0.2.5",
	"private": true,
	"scripts": {
		"tauri": "tauri",
		"dev": "vite dev",
		"dev:backend": "cd src-backend && uvicorn server:app --reload --port 8135",
		"build": "vite build",
		"build:fastapi": "pyinstaller --collect-all en_core_web_sm -c -F --clean --name api-$(rustc -Vv | grep host | cut -f2 -d' ') --distpath src-tauri/bin src-backend/server.py",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"preinstall": "npx only-allow pnpm",
		"test": "vitest",
		"prepare": "husky"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.30.0",
		"@iconify-json/lucide": "^1.2.54",
		"@iconify-json/mdi": "^1.2.3",
		"@iconify-json/ph": "^1.2.2",
		"@internationalized/date": "^3.8.2",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@tauri-apps/api": "^2.6.0",
		"@tauri-apps/cli": "^2.6.2",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "^14.6.1",
		"bits-ui": "^2.8.10",
		"eslint": "^9.30.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.10.1",
		"globals": "^16.2.0",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"svelte": "^5.34.9",
		"svelte-check": "^4.2.2",
		"tailwindcss": "^4.1.11",
		"tailwindcss-animate": "^1.0.7",
		"tslib": "^2.8.1",
